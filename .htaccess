RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www.)?prakted.ml$
RewriteCond %{REQUEST_URI} !^/public/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /public/$1
RewriteCond %{HTTP_HOST} ^(www.)?prakted.ml$
RewriteRule ^(/)?$ public/index.php [L]

<Files .env>
  Order allow,deny
  Deny from all
</Files>
